{
  "name": "MERN_starter",
  "version": "1.0.0",
  "description": "MERN starter",
  "scripts": {
    "start": "node ./server/server.js",
    "dev": "concurrently \"npm start\" \"webpack -d --config webpack.config.js --watch\"",
    "build:dev": "webpack -d --config webpack.config.js --define process.env.NODE_ENV='\"development\"'",
    "build:prod": "webpack -p --config webpack.config.js --define process.env.NODE_ENV='\"production\"'"
  },
  "keywords": [],
  "author": "Qcza",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^3.5.0",
    "express": "^4.15.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0"
  }
}
